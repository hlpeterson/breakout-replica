//Checks if any non-destroyed bricks are in contact with the ball and sets them to destroyed
function findBrick() {
    if(brick1Destroyed == false) {
        if(Math.abs(getBot("#name", "brick1").tags.homeX - ball.tags.homeX) < .9 & Math.abs(getBot("#name", "brick1").tags.homeY - ball.tags.homeY) < .9) {
        //thisBot.tags.label = getBot("#name", "brick1").tags.homeX.toString();
        //destroy(getBot("#name", "brick1")); 
            getBot(byTag("#name", "brick1")).tags.homeX = 100;
            getBot(byTag("#name", "brick1")).tags.color = "black";
            getBot(byTag("#name", "brick1")).tags.scaleX = .1;
            getBot(byTag("#name", "brick1")).tags.scaleY = .1;
            brick1Destroyed = true;
            bricksDestroyed = bricksDestroyed + 1; 
            if(getBot("#name", "brick1").tags.homeX < ball.tags.homeX && getBot("#name", "brick1").tags.homeY < ball.tags.homeY) {
                return "right";
            } else if(getBot("#name", "brick1").tags.homeX < ball.tags.homeX && getBot("#name", "brick1").tags.homeY > ball.tags.homeY) {
                return "bottom";
            } else if(getBot("#name", "brick1").tags.homeX > ball.tags.homeX && getBot("#name", "brick1").tags.homeY > ball.tags.homeY) {
                return "left"; 
            } else {
                return "top"; 
            }
        }
    } 
    if(brick2Destroyed == false) {
        if(Math.abs(getBot("#name", "brick2").tags.homeX - ball.tags.homeX) < .9 & Math.abs(getBot("#name", "brick2").tags.homeY - ball.tags.homeY) < .9) {
        //thisBot.tags.label = getBot("#name", "brick2").tags.homeX.toString();
        //destroy(getBot("#name", "brick2"));
        getBot(byTag("#name", "brick2")).tags.homeX = 100;
        getBot(byTag("#name", "brick2")).tags.color = "black";
        getBot(byTag("#name", "brick2")).tags.scaleX = .1;
        getBot(byTag("#name", "brick2")).tags.scaleY = .1;
        brick2Destroyed = true;
        bricksDestroyed = bricksDestroyed + 1;
        if(getBot("#name", "brick2").tags.homeX < ball.tags.homeX && getBot("#name", "brick2").tags.homeY < ball.tags.homeY) {
                return "right";
            } else if(getBot("#name", "brick2").tags.homeX < ball.tags.homeX && getBot("#name", "brick2").tags.homeY > ball.tags.homeY) {
                return "bottom";
            } else if(getBot("#name", "brick2").tags.homeX > ball.tags.homeX && getBot("#name", "brick2").tags.homeY > ball.tags.homeY) {
                return "left"; 
            } else {
                return "top"; 
            }
        }
    }
    if(brick3Destroyed == false) {
