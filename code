//checks to see if the ball is touching the left boundary
function isTouchingLeft() {
    if(ball.tags.homeX * -1 - 12.4 > ball.tags.homeY) {
        return true;
    } else {
        return false;
    }
}
//checks to see if the ball is touching the top boundary
function isTouchingTop() {
    if(ball.tags.homeX + 27.7 < ball.tags.homeY) {
        return true;
    } else {
        return false;
    }
}
//checks to see if the ball is touching the right boundary
function isTouchingRight() {
    if(ball.tags.homeX * -1 + 12 < ball.tags.homeY) {
        return true;
    } else {
        return false;
    }
}
//checks to see if the ball is touching the bottom boundary
function isTouchingBottom() {
    if(ball.tags.homeX + .4 > ball.tags.homeY) {
        return true;
    } else {
        return false;
    }
}

let leftBoundary = false;
let topBoundary = true;
let rightBoundary = true;
let bottomBoundary = true; 
let pointer = "left"; 
let recentLR = "left";
let recentTB = "bottom"; 
let brickSide = "left"; 
let clockwise = true; 
let secondToLast = "bottom"; 
//Sets the time intervals for the game
let interval1 = setInterval(function() {   
    if(bricksDestroyed >= 4) {
        clearInterval(interval1); 
        let interval2 = setInterval(function() {   
            if(bricksDestroyed > 7) {
                clearInterval(interval2);
                let interval3 = setInterval(function() {   
    if(bricksDestroyed > 31) {
        clearInterval(interval3); 
        gameOver.tags.homeX = -4.8;
        gameOver.tags.homeY = 4.5;
        gameOver.tags.scaleX = 10;
        gameOver.tags.scaleY = 2;
        gameOver.tags.color = "white";
        gameOver.tags.label = "Congratulations! You destroyed all of the bricks";
        ball.tags.homeX = 100;
        ball.tags.homeY = 100;
        ball.tags.color = "black";
        getBot(byTag("#name", "playAgain")).tags.scaleX = 10;
        getBot(byTag("#name", "playAgain")).tags.scaleY = 2;
        getBot(byTag("#name", "playAgain")).tags.homeX = -2.0284150293304037;
        getBot(byTag("#name", "playAgain")).tags.homeY = 1.7109281484096428;
        getBot(byTag("#name", "playAgain")).tags.color = "grey";
    } 
    if(getBot(byTag("#name", "playAgain")).tags.color == "grey") {
        clearInterval(interval3);
    }
    gameState();
}, 10)
            }
    if(getBot(byTag("#name", "playAgain")).tags.color == "grey") {
        clearInterval(interval3);
    }
    gameState(); 
}, 15)
    }
    if(getBot(byTag("#name", "playAgain")).tags.color == "grey") {
        clearInterval(interval3);
    }
    gameState(); 
}, 20)

//Runs through operations to track the gamestate
function gameState() {
    thisBot.tags.label = " "; 
    if(!isTouchingLeft() && isTouchingTop() && leftBoundary == false && (!isTouchingBricks() || isDestroyed())) {
            ball.tags.homeY = ball.tags.homeY - .1;
            clockwise = false; 
    } else if(!isTouchingLeft() && isTouchingBottom() && leftBoundary == false && (!isTouchingBricks() || isDestroyed())) {
        if(Math.abs(getBot("#color", "green").tags.homeX - ball.tags.homeX) < .9 && Math.abs(getBot("#color", "green").tags.homeY  - ball.tags.homeY) < .9) {
            ball.tags.homeX = ball.tags.homeX - .1;
            clockwise = true;
        } else {
            gameOver.tags.homeX = -4.8;
            gameOver.tags.homeY = 4.5;
            gameOver.tags.scaleX = 10;
            gameOver.tags.scaleY = 2;
            gameOver.tags.color = "white";
            gameOver.tags.label = "Game over. You destroyed " + bricksDestroyed.toString() + " bricks";
            ball.tags.homeX = 100;
            ball.tags.homeY = 100;
            ball.tags.color = "black"; 
            pointer = "done"; 
            getBot(byTag("#name", "playAgain")).tags.scaleX = 10;
            getBot(byTag("#name", "playAgain")).tags.scaleY = 2;
            getBot(byTag("#name", "playAgain")).tags.homeX = -2.0284150293304037;
            getBot(byTag("#name", "playAgain")).tags.homeY = 1.7109281484096428;
            getBot(byTag("#name", "playAgain")).tags.color = "grey" 
        }
    } else if (!isTouchingLeft() && leftBoundary == false && (!isTouchingBricks() || isDestroyed())) {
        if(clockwise == true) {
            ball.tags.homeX = ball.tags.homeX - .1;
        } else {
            ball.tags.homeY = ball.tags.homeY - .1;
        }
    } else if(isTouchingBricks() && !isDestroyed() && pointer == "left") { 
        brickSide = findBrick(); 
        leftBoundary = true;
        if(brickSide == "right" && recentTB == "bottom") {
            topBoundary = false;
            pointer = "top"; 
            clockwise = true; 
        } else if(brickSide == "right" && recentTB == "top") {
            bottomBoundary = false;
            pointer = "bottom"; 
            clockwise = false;
        } else if(brickSide == "top") {
            topBoundary = false;
            pointer = "top"; 
            clockwise = false;
        } else if(brickSide == "bottom") { 
            bottomBoundary = false;
            pointer = "bottom"; 
            clockwise = true;
        }
    } else if(pointer == "left"){
        if(clockwise == true) {
            topBoundary = false;
            pointer = "top"; 
        } else {
            bottomBoundary = false;
            pointer = "bottom"; 
        }
        leftBoundary = true;
        recentLR = "left";
    }


    if(!isTouchingTop() && isTouchingLeft() && topBoundary == false && (!isTouchingBricks() || isDestroyed())) {
        ball.tags.homeY = ball.tags.homeY + .1;
        clockwise = true;
    } else if(!isTouchingTop() && isTouchingRight() && topBoundary == false && (!isTouchingBricks() || isDestroyed())) {
        ball.tags.homeX = ball.tags.homeX - .1;
        clockwise = false;
    } else if(!isTouchingTop() && topBoundary == false && (!isTouchingBricks() || isDestroyed())) {
        if(clockwise == true) {
            ball.tags.homeY = ball.tags.homeY + .1;
        } else {
            ball.tags.homeX = ball.tags.homeX - .1;
        }
    } else if(isTouchingBricks() && !isDestroyed() && pointer == "top") {
        brickSide = findBrick();
        topBoundary = true;
        if(brickSide == "bottom" && recentLR == "left") {
            rightBoundary = false;
            pointer = "right"; 
            clockwise = true;
        } else if(brickSide == "bottom" && recentLR == "right") {
            leftBoundary = false;
            pointer = "left"; 
            clockwise = false;
        } else if(brickSide == "right") {
            rightBoundary = false;
            pointer = "right"; 
            clockwise = false;
        } else if(brickSide == "left") {
            leftBoundary = false;
            pointer = "left"; 
            clockwise = true;
        }
    } else if(pointer == "top") { 
        if(clockwise) {
            rightBoundary = false;
            pointer = "right";
        } else {
            leftBoundary = false;
            pointer = "left";
        }
        topBoundary = true;
        recentTB = "top"; 
    }


    if(!isTouchingRight() && isTouchingTop() && rightBoundary == false && (!isTouchingBricks() || isDestroyed())) {
        ball.tags.homeX = ball.tags.homeX + .1;
        clockwise = true;
    } else if(!isTouchingRight() && isTouchingBottom() && rightBoundary == false && (!isTouchingBricks() || isDestroyed())) {
        ball.tags.homeY = ball.tags.homeY + .1;
        clockwise = false;
    } else if(!isTouchingRight() && rightBoundary == false && (!isTouchingBricks() || isDestroyed())) {
        if(clockwise) {
            ball.tags.homeX = ball.tags.homeX + .1;
        } else {
            ball.tags.homeY = ball.tags.homeY + .1;
        }
    } else if(isTouchingBricks() && !isDestroyed() && pointer == "right") { 
        brickSide = findBrick();
        rightBoundary = true;
        if(brickSide == "left" && recentTB == "top") {
            topBoundary = false;
            pointer = "top"; 
            clockwise = false;
        } else if(brickSide == "left" && recentTB == "bottom") {
            bottomBoundary = false;
            pointer = "bottom"; 
            clockwise = true;
        } else if(brickSide == "top") {
            topBoundary = false;
            pointer = "top"; 
            clockwise = true;
        } else if(brickSide == "bottom") {
            bottomBoundary = false;
            pointer = "bottom"; 
            clockwise = false;
        }
    } else if(pointer == "right") {
        if(clockwise) {
            bottomBoundary = false;
            pointer = "bottom"; 
        } else {
            topBoundary = false;
            pointer = "top"; 
        }
        rightBoundary = true;
        recentLR = "right";
    }


    
    if(!isTouchingBottom() && isTouchingLeft() && bottomBoundary == false && (!isTouchingBricks() || isDestroyed())) {
        ball.tags.homeX = ball.tags.homeX + .1;
        clockwise = false;
    } else if(!isTouchingBottom() && isTouchingRight() && bottomBoundary == false && (!isTouchingBricks() || isDestroyed())) {
        ball.tags.homeY = ball.tags.homeY - .1;
        clockwise = true;
    } else if(!isTouchingBottom() && bottomBoundary == false && (!isTouchingBricks() || isDestroyed())) {
        if(clockwise) {
            ball.tags.homeY = ball.tags.homeY - .1; 
        } else { 
            ball.tags.homeX = ball.tags.homeX + .1;
        }
    } else if(isTouchingBricks() && !isDestroyed() && pointer == "bottom") {
        brickSide = findBrick();
        bottomBoundary = true;
        if(brickSide == "top" && recentLR == "right") {
            rightBoundary = false;
            pointer = "right"; 
            clockwise = false;
        } else if(brickSide == "top" && recentLR == "left") {
            leftBoundary = false;
            pointer = "left"; 
            clockwise = true;
        } else if(brickSide == "right") {
            rightBoundary = false;
            pointer = "right"; 
            clockwise = true;
        } else if(brickSide == "left") {
            leftBoundary = false;
            pointer = "left"; 
            clockwise = false;
        }
    } else if(pointer == "bottom" && Math.abs(getBot("#color", "green").tags.homeX - ball.tags.homeX) < .9 && Math.abs(getBot("#color", "green").tags.homeY  - ball.tags.homeY) < .9) {
        if(getBot("#color", "green").tags.homeX > ball.tags.homeX || getBot("#color", "green").tags.homeY > ball.tags.homeY) {
            leftBoundary = false;
            pointer = "left"; 
            if(!clockwise) {
                clockwise = true; 
            }
        } else {
            rightBoundary = false;
            pointer = "right"; 
            if(clockwise) {
                clockwise = false;
            } 
        }
        bottomBoundary = true;
        recentTB = "bottom";
    } else if(pointer == "bottom") { 
        gameOver.tags.homeX = -4.8;
        gameOver.tags.homeY = 4.5;
        gameOver.tags.scaleX = 10;
        gameOver.tags.scaleY = 2;
        gameOver.tags.color = "white";
        gameOver.tags.label = "Game over. You destroyed " + bricksDestroyed.toString() + " bricks";
        ball.tags.homeX = 100;
        ball.tags.homeY = 100;
        ball.tags.color = "black"; 
        pointer = "done"; 
        getBot(byTag("#name", "playAgain")).tags.scaleX = 10;
        getBot(byTag("#name", "playAgain")).tags.scaleY = 2;
        getBot(byTag("#name", "playAgain")).tags.homeX = -2.0284150293304037;
        getBot(byTag("#name", "playAgain")).tags.homeY = 1.7109281484096428;
        getBot(byTag("#name", "playAgain")).tags.color = "grey";
    }
}
